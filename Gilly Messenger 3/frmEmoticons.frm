Attribute VB_Name = "frmEmoticons"
Attribute VB_GlobalNameSpace = False
Attribute VB_Creatable = False
Attribute VB_PredeclaredId = True
Attribute VB_Exposed = False
Option Explicit

Public SrcBox As TextBox
Private LastIcon As Integer

Private Sub Form_KeyDown(KeyCode As Integer, Shift As Integer)
    LastActive = Timer
End Sub

Private Sub Form_KeyPress(KeyAscii As Integer)
    If KeyAscii = vbKeyEscape Then
        Call HideEmoticons
    End If
End Sub

Private Sub Form_Load()
    If Not Transparency = 0 Then
        SetTransparency Me, Transparency
    End If
End Sub

Private Sub Form_LostFocus()
    Call HideEmoticons
End Sub

Private Sub Form_MouseMove(Button As Integer, Shift As Integer, x As Single, y As Single)
    LastActive = Timer
End Sub

Private Sub imgEmoticon_Click(Index As Integer)
    SrcBox.SelText = imgEmoticon(Index).Tag
    Call HideEmoticons
End Sub

Private Sub imgEmoticon_MouseMove(Index As Integer, Button As Integer, Shift As Integer, x As Single, y As Single)
    If LastIcon <> Index Then
        imgEmoticon(LastIcon).BorderStyle = vbBSNone
        imgEmoticon(Index).BorderStyle = vbFixedSingle
        LastIcon = Index
    End If
End Sub

Public Sub HideEmoticons()
    On Error Resume Next
    
    Me.Visible = False
    imgEmoticon(LastIcon).BorderStyle = vbBSNone
    LastIcon = 0
End Sub
